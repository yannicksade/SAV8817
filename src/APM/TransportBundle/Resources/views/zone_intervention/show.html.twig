{% extends "APMAdminBundle:_Main:layout.html.twig" %}

{% block body %}
    <h1>Zone_intervention</h1>

    <table>
        <tbody>
        <tr>
            <th>Code</th>
            <td>{{ zone_intervention.code }}</td>
        </tr>
        <tr>
            <th>Description</th>
            <td>{{ zone_intervention.designation }}</td>
        </tr>
        <tr>
            <th>Langue</th>
            <td>{{ zone_intervention.language }}</td>
        </tr>
        <tr>
            <th>Nom</th>
            <td>{{ zone_intervention.description }}</td>
        </tr>
        <tr>
            <th>Pays</th>
            <td>{{ zone_intervention.pays }}</td>
        </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('apm_zone_intervention_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('apm_zone_intervention_edit', { 'id': zone_intervention.id }) }}">Edit</a>
        </li>
        <li>
            <a href="{{ path('apm_zone_intervention_new') }}">Add a new one</a>
        </li>

        <li>
            {{ form_start(delete_form) }}
            <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}

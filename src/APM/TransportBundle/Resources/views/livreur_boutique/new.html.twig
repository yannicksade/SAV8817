{% extends "APMAdminBundle:_Main:layout.html.twig" %}

{% block body %}
    <h1> Création d'un Livreur pour la boutique {{ boutique }} </h1>

    {{ form_start(form) }}
    {{ form_widget(form) }}
    <input type="submit" value="Create"/>
    {{ form_end(form) }}

    <ul>
        <li>
            <a href="{{ path('apm_transport_livreur_boutique_index', {'id':boutique.id}) }}">Retour à la liste des
                livreurs boutique</a>
        </li>
    </ul>
{% endblock %}

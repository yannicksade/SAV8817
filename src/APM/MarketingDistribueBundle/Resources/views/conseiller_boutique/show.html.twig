{% extends "APMAdminBundle:_Main:layout.html.twig" %}

{% block body %}
    <h1>Conseiller_boutique</h1>

    <table>
        <tbody>

        <tr>
            <th>User Name:</th>
            <td>{{ conseiller_boutique.conseiller.utilisateur.username }}</td>
        </tr>
        <tr>
            <th>Boutique:</th>
            <td>{{ conseiller_boutique.boutique.designation }}</td>
        </tr>
        <tr>
            <th>Conseiller:</th>
            <td>{{ conseiller_boutique.conseiller.code }}</td>
        </tr>

        <tr>
            <th>Valeurs:</th>
            <td>{{ conseiller_boutique.gainValeur }}</td>
        </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('apm_marketing_conseiller_boutique_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('apm_marketing_conseiller_boutique_edit', { 'id': conseiller_boutique.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
            <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}

{% extends 'APMAdminBundle:_Main:layout.html.twig' %}

{% block body %}
    <h1>Service_apres_vente</h1>

    <table>
        <tbody>
        <tr>
            <th>Codesav</th>
            <td>{{ service_apres_vente.codeSav }}</td>
        </tr>
        <tr>
            <th>Date</th>
            <td>{% if service_apres_vente.dateDue %}{{ service_apres_vente.dateDue|date('Y-m-d H:i:s') }}{% endif %}</td>
        </tr>
        <tr>
            <th>Descriptionpanne</th>
            <td>{{ service_apres_vente.descriptionPanne }}</td>
        </tr>
        <tr>
            <th>Etat</th>
            <td>{{ service_apres_vente.etat }}</td>
        </tr>
        <tr>
            <th>Id</th>
            <td>{{ service_apres_vente.id }}</td>
        </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('apm_achat_service_apres_vente_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('apm_achat_service_apres_vente_edit', { 'id': service_apres_vente.id }) }}">Edit</a>
        </li>
        <li>
            <a href="{{ path('apm_achat_service_apres_vente_new') }}">Add a new one</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
            <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}

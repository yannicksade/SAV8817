{% extends "APMAdminBundle:_Main:layout.html.twig" %}

{% block body %}
    <h1>Groupe_relationnel</h1>

    <table>
        <tbody>
        <tr>
            <th>Description</th>
            <td>{{ groupe_relationnel.description }}</td>
        </tr>
        <tr>
            <th>Nom</th>
            <td>{{ groupe_relationnel.designation }}</td>
        </tr>
        <tr>
            <th>Type</th>
            <td>{{ groupe_relationnel.type }}</td>
        </tr>
        <tr>
            <th>Code</th>
            <td>{{ groupe_relationnel.code }}</td>
        </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('apm_user_groupe-relationnel_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('apm_user_groupe-relationnel_edit', { 'id': groupe_relationnel.id }) }}">Edit</a>
        </li>
        <li>
            <a href="{{ path('apm_user_groupe-relationnel_new') }}">Add a new one</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
            <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}

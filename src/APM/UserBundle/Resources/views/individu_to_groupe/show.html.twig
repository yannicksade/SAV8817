{% extends "APMAdminBundle:_Main:layout.html.twig" %}

{% block body %}
    <h1>Individu_to_groupe</h1>

    <table>
        <tbody>
        <tr>
            <th>Propriete</th>
            <td>{{ individu_to_groupe.propriete }}</td>
        </tr>
        <tr>
            <th>Code</th>
            <td>{{ individu_to_groupe.code }}</td>
        </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('apm_user_individu-to-groupe_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('apm_user_individu-to-groupe_edit', { 'id': individu_to_groupe.id }) }}">Edit</a>
        </li>
        <li>
            <a href="{{ path('apm_user_individu-to-groupe_new') }}">Add a new one</a>
        </li>

        <li>
            {{ form_start(delete_form) }}
            <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}

               <div class="col-sm-3" style="padding-right: 0">
                  {% include "::base/platform/Template/left_menu_categorie.html.twig" %}
                  <hr>
                  {% set sup_prod = 1 %}
                  {% include "::base/platform/Template/filtre_sup.html.twig" %}
